<% form_for @user_session do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :password %><br />
    <%= f.password_field :password %>
  </p>
  <p><%= f.submit "Login" %></p>
  <p>
    Forgot password? <%= link_to 'Reset here', new_password_reset_path %>
  </p>
<% end %>


